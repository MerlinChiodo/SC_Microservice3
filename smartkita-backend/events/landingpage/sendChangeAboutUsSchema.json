{
  "title": "Change About Us",
  "description": "Schema for the event to change the about us",
  "type": "object",
  "properties": {
    "event_id" : {
      "type": "integer",
      "minimum": 0,
      "maximum": 10000
    },
    "event_name": {
      "type": "string",
      "const": "Updated About US"
    },
    "service_name": {
      "type": "string",
      "const": "kita"
    },
    "date": {
      "type": "string"
    },
    "about_us":{
      "type": "string"
    },
    "picture": {
      "type": "string"
    }
